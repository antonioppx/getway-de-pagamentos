{
  "version": 2,
  "builds": [
    {
      "src": "src/server-vercel.ts",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run build:vercel"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server-vercel.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "3000"
  }
}
